{
	"name": "rapidcds",
	"displayName": "rapidcds",
	"description": "Additional support for rapid developing in CAP CDS",
	"version": "0.0.1",
	"author": {
		"name": "Anton Kudyrka aka zkud",
		"email": "anton.kudyrka@gmail.com",
		"url": "https://github.com/zkud"
	},
	"maintainers": [
		{
			"name": "Anton Kudyrka aka zkud",
			"email": "anton.kudyrka@gmail.com",
			"url": "https://github.com/zkud"
		},
		{
			"name": "Stsiapan Saveliev aka steaven",
			"email": ":blank:",
			"url": ":blank:"
		}
	],
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Snippets",
		"Programming Languages"
	],
	"activationEvents": [
		"onCommand:rapidcds.generateMockCsv"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "rapidcds.generateMockCsv",
				"title": "rapid cds: Generate Mock .csv"
			}
		],
		"snippets": [
			{
				"language": "cds",
				"path": "./src/snippets/auth.json"
			},
			{
				"language": "cds",
				"path": "./src/snippets/common.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
