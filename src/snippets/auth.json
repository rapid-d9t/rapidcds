{
    "Service's Entity Auth Annotation": {
        "prefix": ["annotate", "annotate-entity"],
        "body": [
            "annotate ${0:ServiceName}.${1:EntityName} with @(restrict: [",
            "\t{ grant: 'READ', to: '${2:ReadScopeName}' },",
            "\t{ grant: '*', to: '${3:EditScopeName}' }",
            "]);"
        ],
        "desciption": "rapidcds: Auth Annotations for Entity of Service"
    }
}